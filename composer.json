{
    "name": "larva/laravel-wechat-notification-channel",
    "description": "This is a Laravel expansion for the wechat Push.",
    "keywords": [
        "Larva",
        "laravel",
        "wechat"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.1 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/notifications": "^5.1 || ^6.0 || ^7.0 || ^8.0",
        "larva/supports": "^2.0",
        "overtrue/laravel-wechat": "^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Larva\\WeChat\\Notifications\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Larva\\WeChat\\Notifications\\WeChatChannelServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
